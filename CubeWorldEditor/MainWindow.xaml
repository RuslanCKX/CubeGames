<Window x:Name="frmMain" x:Class="CubeWorldEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Cube World Editor" Height="600" Width="800" ResizeMode="NoResize" Loaded="frmMain_Loaded">
    <Grid x:Name="gridMain" Background="#FFE5E5E5">
        <TabControl HorizontalAlignment="Left" Height="519" Margin="0,42,0,0" VerticalAlignment="Top" Width="784">
            <TabItem Header="Основные свойтсва">
                <Grid Background="#FFE5E5E5" Margin="0,0,0,0"/>
            </TabItem>
            <TabItem Header="TabItem">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <ToolBar HorizontalAlignment="Left" VerticalAlignment="Top" Height="37" Width="783">
            <Button x:Name="cmdSetGamePatch" Height="32" VerticalAlignment="Top" Width="31">
                <Image Height="32" Width="40" Source="Icons/up32.png"/>
            </Button>
        </ToolBar>

    </Grid>
</Window>
